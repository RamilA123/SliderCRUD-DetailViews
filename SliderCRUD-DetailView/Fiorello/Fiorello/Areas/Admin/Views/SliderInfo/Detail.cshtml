@model SliderInfoDetailVM
@{
    ViewData["Title"] = "Detail";
}



<div class="container my-3">
    <h1 class="text-center">Slider detail</h1>
    <div class="slider-detail">
        <h3>@Html.Raw(Model.Title)</h3>
        <img src="~/img/@Model.SignImage" alt="image" style="width:400px;height:100px;" />
        <h3 class="my-3">Create date : @Model.CreateDate</h3>
        <h3>Info : @Model.Info </h3>
    </div>
    <a asp-action="Index" class="btn btn-primary btn-rounded btn-fw">Back</a>

</div>